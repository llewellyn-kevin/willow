{
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^10.14",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "limedeck/phpunit-detailed-printer": "^6.0",
        "phpstan/phpstan": "^0.12.91"
    },
    "autoload": {
        "psr-4": {
            "Willow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Willow\\ServiceProvider"
            ]
        }
    }
}
